{
  "name": "full-stack-template",
  "version": "1.0.0",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build-client-dev": "set NODE_ENV=development&& webpack",
    "build-client-prod": "set NODE_ENV=production&& webpack",
    "watch-client-dev": "set NODE_ENV=development&& webpack --watch",
    "watch-client-prod": "set NODE_ENV=production&& webpack --watch",
    "build-server": "npm run clean-server && dotnet publish src/Frontend -o ../../build/Frontend -c release",
    "start": "dotnet build/Frontend/Frontend.dll",
    "start-client": "webpack-dev-server",
    "clean-server": "rimraf build/Frontend",
    "docker-build": "docker build -t mwinkler/fullstackdemo ./",
    "docker-run": "docker run -p 5001:80 -i -t --name fullstackdemo mwinkler/fullstackdemo",
    "docker-stop": "docker stop fullstackdemo && docker rm fullstackdemo"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.40",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.6",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
