{
  "name": "full-stack-template",
  "version": "1.0.0",
  "author": "",
  "license": "ISC",
  "scripts": {
    "build-client__dev": "set NODE_ENV=development&& webpack",
    "build-client__dev:watch": "set NODE_ENV=development&& webpack --watch",
    "build-client__prod": "set NODE_ENV=production&& webpack",
    "build-client__prod:watch": "set NODE_ENV=production&& webpack --watch",
    "build-server": "rimraf build/Frontend && dotnet publish src/Frontend -o ../../build/Frontend -c release",
    "build-server-docker": "docker build -t mwinkler/fullstackdemo ./",

    "start-builded": "cd build/Frontend && dotnet Frontend.dll",
    "start-builded-docker": "docker run -p 5001:80 -i -t --name fullstackdemo mwinkler/fullstackdemo && docker rm fullstackdemo",
    "start-database-docker": "docker run -e 'ACCEPT_EULA=Y' -e 'SA_PASSWORD=TestPassw0rd' -p 1433:1433 -d microsoft/mssql-server-linux"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "@types/jquery": "^2.0.40",
    "font-awesome": "^4.7.0",
    "react": "^15.4.2",
    "react-dom": "15.4.2",
    "@types/react": "",
    "@types/react-dom": "",
    "material-ui": "0.17.1",
    "@types/material-ui": "",
    "roboto-fontface": "0.7.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.6",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
